<h1>Productos</h1>

<div class="contenedorProductos">
    {{#each productos}}
        <div class="card">
            <p> {{this.title}} </p>
            <p> {{this.description}} </p>
            <p> {{this.price}} </p>
            <button> Comprar </button>
        </div>
    {{/each}}
</div>